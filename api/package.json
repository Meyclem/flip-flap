{
  "name": "flip-flap-api",
  "version": "0.1.0",
  "description": "Feature flag management system API",
  "author": "Meyclem",
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "progressive-rollout",
    "typescript",
    "express",
    "mongodb"
  ],
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo src/types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest run --coverage --watch"
  },
  "dependencies": {
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "mongoose": "8.19.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@stylistic/eslint-plugin": "5.5.0",
    "@types/express": "5.0.3",
    "@types/node": "24.9.1",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "9.38.0",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.4.0",
    "mongodb-memory-server": "10.2.3",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
